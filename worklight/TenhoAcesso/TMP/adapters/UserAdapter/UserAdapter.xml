<wl:adapter name="UserAdapter"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:wl="http://www.worklight.com/integration"
	xmlns:node="http://www.worklight.com/integration/node">

	<displayName>UserAdapter</displayName>
	<description>UserAdapter</description>
	<connectivity>
		<connectionPolicy xsi:type="node:NodeHTTPConnectionPolicyType">
			<!-- An optional parameter to run the adapter in debug mode on the specified port  
			<debugPort>5838</debugPort>
			-->
		</connectionPolicy>
		<loadConstraints maxConcurrentConnectionsPerNode="20" />
	</connectivity>

	<procedure name="getUserData"/>
</wl:adapter>
